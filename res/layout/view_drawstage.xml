<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/gray_pro_bg_title"
            android:padding="@dimen/pro_marginall" >

            <TextView
                android:id="@+id/textView1"
                style="@style/pro_tv_style"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:drawableLeft="@drawable/pro_jian_l"
                android:text="设计阶段"
                android:textColor="@color/gray_pro_txt" />

            <TextView
                android:id="@+id/textView1"
                style="@style/pro_tv_style"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:drawableRight="@drawable/pro_jian_r"
                android:text="地平"
                android:textColor="@color/gray_pro_txt" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="@dimen/pro_marginall" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                android:id="@+id/tv_ownerunit"
                style="@style/pro_tv_style"
                android:drawableRight="@drawable/pro_add_lv"
                android:text="业主单位" />

                <LinearLayout
                    android:id="@+id/layout_contacts"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >
                </LinearLayout>
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <TextView
                android:id="@+id/tv_starttime"
                style="@style/pro_tv_style"
                android:text="预计开工时间"
                android:textColor="@color/gray_pro_txt" />

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <TextView
                android:id="@+id/tv_endtime"
                style="@style/pro_tv_style"
                android:text="预计竣工时间"
                android:textColor="@color/gray_pro_txt" />

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/pro_margin_topbom"
                android:layout_marginTop="@dimen/pro_margin_topbom" >

                <TextView
                    android:id="@+id/tv_ownertype"
                    style="@style/pro_tv_style"
                    android:layout_weight="1"
                    android:text="电梯" />

                <CheckBox
                    
                    android:id="@+id/cb_lift"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_prostage_selector"
                    android:text="" />
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/pro_margin_topbom"
                android:layout_marginTop="@dimen/pro_margin_topbom" >

                <TextView
                    android:id="@+id/tv_ownertype"
                    style="@style/pro_tv_style"
                    android:layout_weight="1"
                    android:text="空调" />

                <CheckBox
                    android:id="@+id/cb_airconditioner"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_prostage_selector"
                    android:text="" />
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/pro_margin_topbom"
                android:layout_marginTop="@dimen/pro_margin_topbom" >

                <TextView
                    android:id="@+id/tv_ownertype"
                    style="@style/pro_tv_style"
                    android:layout_weight="1"
                    android:text="供暖方式" />

                <CheckBox
                    android:id="@+id/cb_heatingway"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_prostage_selector"
                    android:text="" />
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/pro_margin_topbom"
                android:layout_marginTop="@dimen/pro_margin_topbom" >

                <TextView
                    android:id="@+id/tv_ownertype"
                    style="@style/pro_tv_style"
                    android:layout_weight="1"
                    android:text="外墙材料" />

                <CheckBox
                    android:id="@+id/cb_wallmaterial"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_prostage_selector"
                    android:text="" />
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/pro_margin_topbom"
                android:layout_marginTop="@dimen/pro_margin_topbom" >

                <TextView
                    android:id="@+id/tv_ownertype"
                    style="@style/pro_tv_style"
                    android:layout_weight="1"
                    android:text="钢结构" />

                <CheckBox
                    android:id="@+id/cb_steelstructure"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/checkbox_prostage_selector"
                    android:text="" />
            </LinearLayout>

            <View
                style="@style/view_line"
                android:layout_marginBottom="@dimen/pro_margintopbom"
                android:layout_marginTop="@dimen/pro_margintopbom"
                android:background="@color/black" />
        </LinearLayout>
    </LinearLayout>

</ScrollView>