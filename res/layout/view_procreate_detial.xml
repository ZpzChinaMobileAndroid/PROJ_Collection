<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/view_detial_title" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pro_detial_marg10"
        android:layout_marginRight="@dimen/pro_detial_marg10"
        android:layout_marginTop="@dimen/pro_detial_marg10" >

        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="预计开工时间"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_expectedStartTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="建筑层高"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_storeyHeight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="外资参与"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_foreignInvestment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pro_detial_marg10"
        android:layout_marginRight="@dimen/pro_detial_marg10"
        android:layout_marginTop="@dimen/pro_detial_marg10"
        android:orientation="horizontal" >

        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="预计竣工时间"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_expectedFinishTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="投资额"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_investment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="建筑面积"
                android:textColor="@color/blue_pro_txt"
                android:textSize="@dimen/activity_16_size" />

            <TextView
                android:id="@+id/tv_areaOfStructure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textColor="@color/gray_detial_txt" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_contacts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/pro_detial_marg10"
        android:orientation="vertical" >

        <include layout="@layout/view_detial_contact" />

        <include layout="@layout/view_detial_contact" />

        <include layout="@layout/view_detial_contact" />
    </LinearLayout>

    <TextView
        android:textSize="@dimen/activity_18_size"
        android:id="@+id/tv_ownerType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/pro_detial_marg10"
        android:drawableLeft="@drawable/ic_earth"
        android:drawablePadding="@dimen/pro_detial_marg10"
        android:gravity="center_vertical"
        android:layout_marginBottom="@dimen/pro_detial_marg10"
        android:text="TextView" />

</LinearLayout>